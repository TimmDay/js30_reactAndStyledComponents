eight = el.offsetHeight\n    const { offsetWidth: width, offsetHeight: height} = hero\n    const {clientX: x, clientY: y} = e\n\n    const xWalk = Math.round((x/width * walk) - walk/2)\n    const yWalk = Math.round((y/height * walk) - walk/2)\n\n    text.style.textShadow=`\n      ${xWalk}px ${yWalk}px 0 rgba(255,0,255,0.7),\n      ${xWalk/3}px ${yWalk/3}px 0 rgba(0,255,255,0.7)\n    `\n  }\n  return (\n    <StyledTextShadowMouseMoveApp onMouseMove={handleShadow}>\n      <h1>Shadowy Title</h1>\n      <GlobalStyle/>\n    </StyledTextShadowMouseMoveApp>\n  )\n}\nexport default TextShadowMouseMoveApp;  ",["157","158"],"/Users/timday/Desktop/js30/src/reportWebVitals.js",[],["159","160"],"/Users/timday/Desktop/js30/src/components/03CssVariablesAndJs/SliderApp.js",[],["161","162"],"/Users/timday/Desktop/js30/src/components/04ArrayMethods/ArrayMethodsEx.js",["163"],"import React, { useState } from 'react';\nimport styled from 'styled-components';\n\nconst StyledArrayMethodsEx = styled.div`\n  color: black;\n`;\n\nconst inventors = [\n  { first: 'Albert', last: 'Einstein', year: 1879, passed: 1955 },\n  { first: 'Isaac', last: 'Newton', year: 1643, passed: 1727 },\n  { first: 'Galileo', last: 'Galilei', year: 1564, passed: 1642 },\n  { first: 'Marie', last: 'Curie', year: 1867, passed: 1934 },\n  { first: 'Johannes', last: 'Kepler', year: 1571, passed: 1630 },\n  { first: 'Nicolaus', last: 'Copernicus', year: 1473, passed: 1543 },\n  { first: 'Max', last: 'Planck', year: 1858, passed: 1947 },\n  { first: 'Katherine', last: 'Blodgett', year: 1898, passed: 1979 },\n  { first: 'Ada', last: 'Lovelace', year: 1815, passed: 1852 },\n  { first: 'Sarah E.', last: 'Goode', year: 1855, passed: 1905 },\n  { first: 'Lise', last: 'Meitner', year: 1878, passed: 1968 },\n  { first: 'Hanna', last: 'HammarstrÃ¶m', year: 1829, passed: 1909 }\n];\n\nconst people = [\n  'Bernhard, Sandra', 'Bethea, Erin', 'Becker, Carl', 'Bentsen, Lloyd', 'Beckett, Samuel', 'Blake, William', 'Berger, Ric', 'Beddoes, Mick', 'Beethoven, Ludwig',\n  'Belloc, Hilaire', 'Begin, Menachem', 'Bellow, Saul', 'Benchley, Robert', 'Blair, Robert', 'Benenson, Peter', 'Benjamin, Walter', 'Berlin, Irving',\n  'Benn, Tony', 'Benson, Leana', 'Bent, Silas', 'Berle, Milton', 'Berry, Halle', 'Biko, Steve', 'Beck, Glenn', 'Bergman, Ingmar', 'Black, Elk', 'Berio, Luciano',\n  'Berne, Eric', 'Berra, Yogi', 'Berry, Wendell', 'Bevan, Aneurin', 'Ben-Gurion, David', 'Bevel, Ken', 'Biden, Joseph', 'Bennington, Chester', 'Bierce, Ambrose',\n  'Billings, Josh', 'Birrell, Augustine', 'Blair, Tony', 'Beecher, Henry', 'Biondo, Frank'\n];\n\n// Array.prototype.filter()\n// 1. Filter the list of inventors for those who were born in the 1500's\nconst ex1 = inventors.filter((inv) => inv.year > 1500 && inv.year < 1600)\n\n// Array.prototype.map()\n// 2. Give us an array of the inventors first and last names\nconst ex2 = inventors.map((inv) => `${inv.first} ${inv.last}`)\n\n// Array.prototype.sort()\n// 3. Sort the inventors by birthdate, oldest to youngest\nconst ex3 = inventors.sort((invA, invB) => invB.year - invA.year)\n\n// Array.prototype.reduce()\n// 4. How many total years did all the inventors live?\nconst ex4 = inventors.reduce((acc,curr) => acc + (curr.passed - curr.year),0)\n\n// 5. Sort the inventors by years lived\n// const ex5 = inventors.sort((a,b) => {\n//   if ((a.passed - a.year) > (b.passed - b.year)) {\n//     return 1\n//   } else {\n//     return -1\n//   }\n// })\nconst ex5 = inventors.sort((a,b) => (a.passed - a.year) - (b.passed - b.year));\n\n\n// 6. create a list of Boulevards in Paris that contain 'de' anywhere in the name\n// https://en.wikipedia.org/wiki/Category:Boulevards_in_Paris\n// const category = document.querySelector('.mw-category')\n// const links = Array.from(category.querySelectorAll('a'))\n// const blvds = links.map(link => link.innerText) \n// const de = blvds.filter(b => b.includes('de'))\n\n// 7. sort Exercise\n// Sort the people alphabetically by last name\nconst ex7 = people.sort((a,b) => a < b ? -1 : 1)\n\n// 8. Reduce Exercise\n// Sum up the instances of each of these\nconst data = ['car', 'car', 'truck', 'truck', 'bike', 'walk', 'car', 'van', 'bike', 'walk', 'car', 'van', 'car', 'truck' ];\nconst tally = data.reduce((obj, item) => {\n  if (!obj[item]) {\n    obj[item] = 0\n  }\n  obj[item]++\n  return obj\n}, {})\nconst ex8 = Object.keys(tally).map(key => `${key}: ${tally[key]}`)\n\nconst ArrayMethodsEx = (props) => {\n  return (\n    <StyledArrayMethodsEx>\n      <p>Exercise 1: Inventors born in 1500s</p>\n      {ex1.map((o) => (\n        <div>{`${o.first} ${o.last}`}</div>\n      ))}\n      <p>Exercise 2: Array of first last</p>\n      {ex2.map((o) => (\n        <div>{o}</div>\n      ))}\n      <p>Exercise 3: oldest to youngest</p>\n      {ex3.map(inv => (\n        <div>{`${inv.year}: ${inv.first} ${inv.last}`}</div>\n      ))}\n\n      <p>Exercise 4: Total years of inventor life</p>\n        <div>{ex4}</div>\n\n      <p>Exercise 5:</p>\n      {ex5.map((inv) => (\n        <div>{`Years Lived: ${inv.passed - inv.year}, ${inv.first} ${inv.last}`}</div>\n      ))}\n\n      <p>Exercise 6:</p>\n        <div>see comented out code </div>\n\n      <p>Exercise 7: alphabetical</p>\n      {ex7.map((p) => (\n        <div>{p}</div>\n      ))}\n\n      <p>Exercise 8: tally instances</p>\n      {ex8.map(item => (<div>{item}</div>))}\n    </StyledArrayMethodsEx>\n  )\n}\nexport default ArrayMethodsEx;",["164","165"],"/Users/timday/Desktop/js30/src/components/17SortWithoutArticles/SortApp.js",[],["166","167"],"/Users/timday/Desktop/js30/src/components/02clock/ClockApp.jsx",[],"/Users/timday/Desktop/js30/src/components/08HTML5Canvas/DashCanvasColorPalette.js",["168"],"import React, { useState } from 'react';\nimport styled from 'styled-components';\n\nconst StyledCanvasColorPalette = styled.div`\n  background: black;\n  display: flex;\n  justify-content: center;\n  flex-wrap: wrap;\n`;\nconst StyledButton = styled.button`\n  background: black;\n  color: white;\n  border-radius: 5px;\n  margin: 10px;\n  padding: 5px 10px;\n  border: 1px solid white;\n  // min-width: 100px;\n\n  & .dccp__selected {\n    background: white;\n    color: black;\n  }\n`\n\nconst CanvasColorPalette = ({ canvasId }) => {\n\n  const handleUpdateColor = (color) => {\n    console.log('handling')\n    const canvas = document.querySelector(`#${canvasId}`)\n    const ctx = canvas.getContext('2d')\n    ctx.strokeStyle = color\n  }\n\n  return (\n    <StyledCanvasColorPalette>\n      <StyledButton\n          onClick={() => handleUpdateColor('blue')}\n        >water potable\n        </StyledButton>\n        <StyledButton\n          className='dccp__selected'\n          onClick={() => handleUpdateColor('green')}\n        >stormwater\n        </StyledButton>\n        <StyledButton\n          onClick={() => handleUpdateColor('yellow')}\n        >gas\n        </StyledButton>\n        <StyledButton\n          onClick={() => handleUpdateColor('orange')}\n        >electric\n        </StyledButton>\n        <StyledButton\n          onClick={() => handleUpdateColor('white')}\n        >communications\n        </StyledButton>\n        <StyledButton\n          onClick={() => handleUpdateColor('red')}\n        >fire\n        </StyledButton>\n        <StyledButton\n          onClick={() => handleUpdateColor('pink')}\n        >unidentified\n        </StyledButton>\n        <StyledButton\n          onClick={() => handleUpdateColor('purple')}\n        >recycled water\n        </StyledButton>\n        {/* https://www.jaybro.com.au/about/colour-codes-spray-paint/ */}\n    </StyledCanvasColorPalette>\n  )\n}\nexport default CanvasColorPalette;","/Users/timday/Desktop/js30/src/components/05FlexPanelsImageGallery/data/galleryData.js",[],"/Users/timday/Desktop/js30/src/components/10HoldShiftChecks/InboxItem.js",["169"],"/Users/timday/Desktop/js30/src/components/02clock/HourMark.jsx",["170"],"import React, { useState } from 'react';\nimport {createUseStyles} from 'react-jss';\n\nconst markDiameter = 0.5\n\nconst useStyles = createUseStyles({\n  hourMark: props => ({\n    height: `${markDiameter}rem`,\n    width: `${markDiameter}rem`,\n    borderRadius: '50%',\n    background: 'black',\n    position: 'absolute',\n    top: '50%',\n    left: '50%',\n    transform: (props.pos) ? `\n      translateX(${(-markDiameter/2) + Math.sin((props.pos)/12*(360)*Math.PI/180)*(props.dia || 7)}rem) \n      translateY(${(-markDiameter/2) - Math.cos((props.pos)/12*(360)*Math.PI/180)*(props.dia || 7)}rem)\n    ` : `\n      translateX(${(-markDiameter/2)}rem) \n      translateY(${(-markDiameter/2)}rem)\n    `\n  })\n})\n\nconst HourMark = (props) => {\n  const el = document.getElementById('clockfaceapp')\nif (el) {\n  const dim = getComputedStyle(el).getPropertyValue('width')\n  console.log(dim)\n}\n  const classes = useStyles(props)\n  // console.log(props)\n\n  return (\n    <div className={classes.hourMark} />\n  )\n}\nexport default HourMark;","/Users/timday/Desktop/js30/src/components/02clock/ClockHand.jsx",[],"/Users/timday/Desktop/js30/src/components/19Webcam/WebcamApp.js",[],"/Users/timday/Desktop/js30/src/components/10HoldShiftChecks/ShiftCheckApp.js",["171"],"/Users/timday/Desktop/js30/src/components/11VideoPlayer/VideoApp.js",[],"/Users/timday/Desktop/js30/src/components/01drums/DrumsApp.jsx",["172"],"import React, { useEffect } from 'react'\nimport Keys from './Keys'\nimport {createUseStyles} from 'react-jss'\n\n\nconst useStyles = createUseStyles({\n  drumsApp: {\n    background: 'url(\"/01background03.jpg\")',\n    backgroundSize: 'cover',\n    margin: 0,\n    padding: 0,\n    fontFamily: 'sans-serif',\n    height: '100vh'\n  }\n})\n\n\nconst DrumsApp = () => {\n  const classes = useStyles()\n\n  useEffect(() => {\n    document.body.className = classes.drumsApp\n    return () => document.body.className = null\n  }, [])\n\n  return (\n    <div className={classes.drumsApp}>\n      <Keys />\n    </div>\n  )\n}\n\nexport { DrumsApp as default }","/Users/timday/Desktop/js30/src/data/10data.js",[],"/Users/timday/Desktop/js30/src/components/02clock/ClockFace.jsx",["173","174"],"/Users/timday/Desktop/js30/src/components/06FetchAndLookup/LookupApp.js",[],"/Users/timday/Desktop/js30/src/utils/utils.js",[],"/Users/timday/Desktop/js30/src/components/08HTML5Canvas/DashLocalStorage.js",["175","176"],"import React from 'react';\nimport styled from 'styled-components';\n\nconst StyledDashLocalStorage = styled.div`\n  // position: absolute;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n\n  & button {\n    margin: 10px;\n    // padding: 5px 10px;\n    width: 50px;\n    height: 40px;\n    border-radius: 50%;\n    background: black;\n    color: white;\n    outline: 0;\n  }\n`;\n\nconst DashLocalStorage = ({ canvasId }) => {\n\n\n  const handleSave = () => {\n    const canvas = document.querySelector(`#${canvasId}`)\n    const ctx = canvas.getContext('2d')\n    localStorage.setItem('canvasName', canvas.toDataURL());\n  }\n\n  const handleLoad = () => {\n    const canvas = document.querySelector(`#${canvasId}`)\n    const ctx = canvas.getContext('2d')\n\n    const dataURL = localStorage.getItem('canvasName');\n    const img = new Image;\n    img.src = dataURL;\n    img.onload = function () {\n      ctx.drawImage(img, 0, 0);\n    };\n  }\n\n  const handleClear = () => {\n    localStorage.clear()\n    window.location.reload()\n  }\n\n  return (\n    <StyledDashLocalStorage>\n      <button\n        onClick={handleSave}\n      >\n        save\n      </button>\n      <button\n        onClick={handleLoad}\n      >\n        load\n      </button>\n      <button\n        onClick={handleClear}\n      >\n        clear\n      </button>\n    </StyledDashLocalStorage>\n  )\n}\nexport default DashLocalStorage;","/Users/timday/Desktop/js30/src/components/01drums/Keys.jsx",["177"],"import React, { useEffect } from 'react'\nimport {createUseStyles} from 'react-jss'\n\nconst useStyles = createUseStyles({\n  playing: {\n    transform: 'scale(2)',\n    borderColor: '#ffc600',\n    boxShadow: '0 0 1rem #ffc600',\n  },\n  keys: {\n    display: 'flex',\n    flexWrap: 'wrap',\n    flex: 1,\n    minHeight: '100vh',\n    alignItems: 'center',\n    justifyContent: 'center'\n  },\n  key: {\n    border: '.4rem solid black',\n    borderRadius: '50%',\n    boxSizing: 'border-box',\n    margin: '1rem',\n    fontSize: '1.5rem',\n    padding: '1rem .5rem',\n    transition: 'all .07s ease',\n    width: '8rem',\n    height: '8rem',\n    textAlign: 'center',\n    color: 'white',\n    background: 'rgba(0.8,0.8,0.8,0.4)',\n    textShadow: '0 0 .5rem black',\n    '& kbd': {\n      display: 'block',\n      fontSize: '3rem',\n      color: 'red'\n    }\n  },\n  sound: {\n    fontSize: '1.2rem',\n    textTransform: 'uppercase',\n    letterSpacing: '.1rem',\n    color: '#ffc600', //yellow\n  }\n})\n\nconst sounds = [\n  [65, \"A\", \"clap\", new Audio(\"sounds/clap.wav\")],\n  [83, \"S\", \"hihat\", new Audio(\"sounds/hihat.wav\")],\n  [68, \"D\", \"kick\", new Audio(\"sounds/kick.wav\")],\n  [70, \"F\", \"openhat\", new Audio(\"sounds/openhat.wav\")],\n  [71, \"G\", \"boom\", new Audio(\"sounds/boom.wav\")],\n  [72, \"H\", \"ride\", new Audio(\"sounds/ride.wav\")],\n  [74, \"J\", \"snare\", new Audio(\"sounds/snare.wav\")],\n  [75, \"K\", \"tom\", new Audio(\"sounds/tom.wav\")],\n  [76, \"L\", \"tink\", new Audio(\"sounds/tink.wav\")]\n];\n\nconst Drums = () => {\n  const classes = useStyles()\n\n  useEffect(() => {\n    const handleKeyDown = (e) => {\n      const sound = sounds.find(sound => sound[0] === e.keyCode) //checks the keycode of pressed key against the list of keycodes\n\n      if (sound) {\n        const el = document.querySelector(`div[data-key='${sound[0]}']`)\n        el.classList.add(classes.playing)\n        sound[3].currentTime = 0 //reset current audio file if already playing\n        sound[3].play()\n      }\n    }\n\n    window.addEventListener('keydown', handleKeyDown)\n    return () => {\n      window.removeEventListener('keydown', handleKeyDown)\n    }\n  },[])\n\n  const removeTransition = e => e.target.classList.remove(classes.playing);\n\n  return (\n    <div className={classes.keys}>\n      {sounds.map(([keyCode, keyName, soundName, ...rest]) => (\n        <div className={classes.key} key={keyCode} data-key={keyCode} onTransitionEnd={removeTransition}>\n          <kbd>{keyName}</kbd>\n          <span className={classes.sound}>{soundName}</span>\n        </div>  \n      ))}\n    </div>\n  )\n}\n\nexport { Drums as default }","/Users/timday/Desktop/js30/src/components/08HTML5Canvas/CanvasApp.js",[],"/Users/timday/Desktop/js30/src/components/05FlexPanelsImageGallery/Panel.js",["178"],"/Users/timday/Desktop/js30/src/components/20SpeechRecognition/SpeechRecognitionApp.js",[],"/Users/timday/Desktop/js30/src/components/00Utilities/ButtonBar.js",[],"/Users/timday/Desktop/js30/src/components/05FlexPanelsImageGallery/GalleryApp.js",["179"],"import React, { useState, useEffect } from 'react';\nimport styled, { createGlobalStyle } from 'styled-components';\nimport Panel from './Panel'\nimport panelData from './data/galleryData'\n\nconst GlobalStyle = createGlobalStyle`\n  html {\n    box-sizing: border-box;\n    background: #ffc600;\n    font-family: 'helvetica neue';\n    font-size: 20px;\n    font-weight: 200;\n  }\n\n  body {\n    margin: 0;\n  }\n\n  *, *:before, *:after {\n    box-sizing: inherit;\n  }\n`;\nconst StyledGalleryApp = styled.main`\n  min-height: 100vh;\n  overflow: hidden;\n  display: flex;\n`;\n\nconst GalleryApp = (props) => {\n  const [ activePanel, setActivePanel ] = useState(-1) // number indicating index of panelData items\n\n  const handleClick = (e, i) => {\n    e.preventDefault()\n    if (i === activePanel) {\n      setActivePanel(-1)\n    } else {\n      setActivePanel(i)\n    }\n  }\n\n  return (\n    <StyledGalleryApp >\n      {panelData.map((el, index) => (\n        <Panel\n          key={`panel${inde